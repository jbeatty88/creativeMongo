<% include partials/header %>


<div id="carouselExampleControls" class="carousel slide" data-interval="false">
    <div class="carousel-inner">
        <div style="background-color: rgba(0, 0, 0, 0.966);" class="carousel-item active">
            <img 
            style="width: 400px; height: 400px; background-color: rgba(0,0,255,0.1);"
            class="d-block rounded mx-auto mt-0"
            id="imgData" 
            src= "../public/images/wan.jpg" 
            alt="First slide">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<div>
    <div class="text-center mt-4" id="commentDiplay">
        {{ name }}
        
    </div>
</div>




<% include partials/footer %>